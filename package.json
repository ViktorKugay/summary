{
  "name": "summary",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dev-server --mode development --open --hot",
    "build": "NODE_ENV=production webpack",
    "start": "ec",
    "test": "jest",
    "typecheck": "tsc -b packages/frontend",
    "lint:css": "stylelint \"packages/*/src/components/**/*.css\" --config ./.stylelintrc-format",
    "lint:js": "eslint 'packages/frontend/src/**/*.{ts,tsx,js}' --cache",
    "lint": "run-p lint:*",
    "format:js": "prettier --write \"packages/*/src/**/*.{ts,tsx,js}\"",
    "format:json": "prettier --write \"packages/*/src/**/*.json\"",
    "format:css": "prettier --write \"packages/*/src/components/**/*.css\" && stylelint --fix \"packages/*/src/components/**/*.css\" --config ./.stylelintrc-format",
    "format": "run-p format:*"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "HOOK=true yarn test",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "linters": {
      "*.css": [
        "prettier --write",
        "stylelint --fix --config ./.stylelintrc-format",
        "git add"
      ],
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "git add"
      ],
      "*.{md,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "packages/frontend/src/styles/reboot.css"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "autolinker": "^3.1.0",
    "classnames": "2.2.6",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dom-loaded": "^1.0.1",
    "helmet": "^3.21.3",
    "identity-obj-proxy": "^3.0.0",
    "notistack": "^0.9.9",
    "raf": "^3.4.1",
    "react": "^16.4.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-jsx-parser": "^1.21.0",
    "react-modal": "^3.8.1",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.22.4",
    "regenerator-runtime": "^0.13.3",
    "smoothscroll": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-typescript": "^7.7.2",
    "@testing-library/react": "^8.0.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.137",
    "@types/node": "^12.7.5",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-jsonschema-form": "^1.6.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.2",
    "css-loader": "^3.2.0",
    "dotenv": "6.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "jest": "^25.1.0",
    "lint-staged": "^8.0.4",
    "nodemon": "^1.18.9",
    "npm-run-all": "4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pino-colada": "^1.4.4",
    "postcss": "^7.0.21",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.1",
    "rimraf": "2.6.2",
    "source-map-explorer": "1.6.0",
    "style-loader": "^1.0.0",
    "stylelint": "^13.2.0",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-media-use-custom-media": "^1.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "yarnhook": "^0.4.0"
  }
}
